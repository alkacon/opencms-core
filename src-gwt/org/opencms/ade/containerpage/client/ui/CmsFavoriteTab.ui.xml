<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:cms="urn:import:org.opencms.gwt.client.ui">
	<ui:with field="resources" type="org.opencms.gwt.client.ui.css.I_CmsLayoutBundle" />
	<ui:with field="containerpage" type="org.opencms.ade.containerpage.client.ui.css.I_CmsLayoutBundle"></ui:with>
	<ui:style>
		.buttonPanel{
			height: 25px;
		    overflow: hidden;
		    position: absolute;
		    right: 0;
		    top: -29px;
		    text-align: right;
		}
		.buttonPanel > div {
			margin-left: 6px;
		}
	</ui:style>
	<g:FlowPanel addStyleNames="{resources.tabbedPanelCss.tabPanel}">
		<cms:CmsScrollPanel ui:field="m_scrollPanel" addStyleNames="{containerpage.containerpageCss.clipboardList} {resources.generalCss.cornerAll}">
			<cms:CmsList ui:field="m_listPanel"  />
		</cms:CmsScrollPanel>
		<g:FlowPanel ui:field="m_buttonEditingPanel" styleName="{style.buttonPanel}"><cms:CmsPushButton ui:field="m_saveButton" addStyleNames="{resources.buttonCss.cmsMinWidth}" /><cms:CmsPushButton ui:field="m_cancelButton" addStyleNames="{resources.buttonCss.cmsMinWidth}" /></g:FlowPanel>
		<g:FlowPanel ui:field="m_buttonUsePanel" styleName="{style.buttonPanel}"><cms:CmsPushButton ui:field="m_editButton" addStyleNames="{resources.buttonCss.cmsMinWidth}" /></g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>
