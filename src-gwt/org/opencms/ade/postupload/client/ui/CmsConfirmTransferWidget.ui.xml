<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:input="urn:import:org.opencms.gwt.client.ui.input"
	xmlns:cms="urn:import:org.opencms.gwt.client.ui"
	xmlns:cmsi="urn:import:org.opencms.gwt.client.ui.input"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="inputstyle" type="org.opencms.gwt.client.ui.css.I_CmsInputLayoutBundle" />
	<ui:with field="messages" type="org.opencms.ade.postupload.client.ui.CmsConfirmTransferWidget.MessageStrings" />
	<ui:style>
		.checkbox, .checkbox *  {
			font-weight: bold !important;
		}
		
		.checkbox { 
			margin-top: 12px;
			margin-left: 4px !important;
		}
		
		.fieldset > * {
			margin-top: 0px !important;
			margin-bottom: 0px !important;
			
			
		} 
		
		.mainlabel {	
			margin-top: 3px;
			margin-left: 4px !important; 
		}
	
	</ui:style>
	<g:FlowPanel>
		<cms:CmsFieldSet legend="" addStyleNames="{style.fieldset}" openerVisible="false">
			<g:HTML ui:field="m_mainLabel"  text="{messages.confirmText}" addStyleNames="{style.mainlabel}" />
			<input:CmsCheckBox ui:field="m_checkBox" visible="false" text="{messages.checkboxText}" addStyleNames="{style.checkbox}"/>
		</cms:CmsFieldSet>
		<g:FlowPanel visible="false">
			<cms:CmsPushButton useMinWidth="true" ui:field="m_okButton" text="{messages.ok}"/>
			<cms:CmsPushButton useMinWidth="true" ui:field="m_cancelButton" text="{messages.cancel}"/>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>