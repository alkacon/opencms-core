<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:cms="urn:import:org.opencms.gwt.client.ui">
	<ui:with field="resources" type="org.opencms.gwt.client.ui.css.I_CmsLayoutBundle" />
	<ui:with field="gallery" type="org.opencms.ade.galleries.client.ui.css.I_CmsLayoutBundle" />
	<g:FlowPanel addStyleNames="{gallery.galleryDialogCss.showParams} {resources.generalCss.cornerAll}">
		<g:HTML ui:field="m_text" styleName="{gallery.galleryDialogCss.paramsText}"></g:HTML>
		<cms:CmsPushButton ui:field="m_button" imageClass="{resources.gwtImages.style.closeIcon}"></cms:CmsPushButton>
	</g:FlowPanel>
</ui:UiBinder>
