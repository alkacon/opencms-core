<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:cms="urn:import:org.opencms.gwt.client.ui">
	<ui:with field="style" type="org.opencms.gwt.client.ui.css.I_CmsLayoutBundle" />
	<g:HTMLPanel styleName="">
		<div class="{style.notificationCss.notificationMessage} {style.generalCss.cornerAll} {style.generalCss.shadow}">
			<cms:CmsPushButton ui:field="m_closeButton" addStyleNames="{style.notificationCss.closeButton}" />
			<div ui:field="m_messageContent" class="{style.notificationCss.messageContent}"/>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 