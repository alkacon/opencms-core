<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.labelBox {
			padding: 10px; 
			margin-top: 5px;
			min-height: 50px; 
		} 
	</ui:style>
	<ui:with field="resources" type="org.opencms.gwt.client.ui.css.I_CmsLayoutBundle" />
	<g:FlowPanel>
		<g:FlowPanel ui:field="m_infoBox" />
		<g:FlowPanel addStyleNames="{style.labelBox} {resources.generalCss.cornerAll} {resources.generalCss.border}">
			<g:Label ui:field="m_label" />
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 