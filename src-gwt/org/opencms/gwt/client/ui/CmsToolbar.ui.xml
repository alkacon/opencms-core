<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:cms="urn:import:org.opencms.gwt.client.ui">
	<ui:with field="images" type="org.opencms.gwt.client.ui.css.I_CmsImageBundle" />
	<ui:with field="resources" type="org.opencms.gwt.client.ui.css.I_CmsLayoutBundle" />
	<g:FlowPanel ui:field="m_panel" styleName="{resources.toolbarCss.toolbar} {resources.generalCss.opencms}">
		<cms:CmsNotificationWidget />
		<g:FlowPanel styleName="{resources.toolbarCss.toolbarBackground}">
			<g:FlowPanel styleName="{resources.toolbarCss.toolbarContent}" >
				<g:FlowPanel ui:field="m_toolbarCenter" styleName="{resources.toolbarCss.toolbarCenter}">
					<g:InlineLabel styleName="{resources.toolbarCss.toolbarLogo}" />
	  			</g:FlowPanel>
				<g:FlowPanel ui:field="m_buttonPanelLeft" styleName="{resources.toolbarCss.toolbarButtonsLeft}"></g:FlowPanel>
			 	<g:FlowPanel ui:field="m_buttonPanelRight" styleName="{resources.toolbarCss.toolbarButtonsRight}"><cms:CmsQuickLauncher ui:field="m_quickLauncher" /><cms:CmsUserInfo ui:field="m_userInfo" /></g:FlowPanel>
			</g:FlowPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 