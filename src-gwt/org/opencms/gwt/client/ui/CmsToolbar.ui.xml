<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:cms="urn:import:org.opencms.gwt.client.ui">
	<ui:with field="images" type="org.opencms.gwt.client.ui.css.I_CmsImageBundle" />
	<ui:with field="resources" type="org.opencms.gwt.client.ui.css.I_CmsLayoutBundle" />
	<g:FlowPanel ui:field="m_panel" styleName="{resources.toolbarCss.toolbar}">
		<cms:CmsNotificationWidget />
		<g:FlowPanel styleName="{resources.toolbarCss.toolbarBackground}">
			<g:HorizontalPanel styleName="{resources.toolbarCss.toolbarContent}" >
				<g:cell horizontalAlignment="ALIGN_LEFT" verticalAlignment="ALIGN_MIDDLE">
					<g:FlowPanel ui:field="m_buttonPanelLeft" styleName="{resources.toolbarCss.toolbarButtonsLeft}"></g:FlowPanel>
			 	</g:cell>
				<g:cell horizontalAlignment="ALIGN_RIGHT" verticalAlignment="ALIGN_MIDDLE">
					<g:FlowPanel ui:field="m_buttonPanelRight" styleName="{resources.toolbarCss.toolbarButtonsRight}"></g:FlowPanel>
				</g:cell>
			</g:HorizontalPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 