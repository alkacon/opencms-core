<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="resources" type="org.opencms.gwt.client.ui.css.I_CmsLayoutBundle" />
	<g:HTML styleName="{resources.highlightCss.highlightBox} {resources.highlightCss.animated} {resources.generalCss.opencms}">
		<div ui:field="m_borderTop" class="{resources.highlightCss.borderTop}"></div>
		<div ui:field="m_borderRight" class="{resources.highlightCss.borderRight}"></div>
		<div ui:field="m_borderBottom" class="{resources.highlightCss.borderBottom}"></div>
		<div ui:field="m_borderLeft" class="{resources.highlightCss.borderLeft}"></div>	
		<div ui:field="m_midpoints"></div>
	</g:HTML>
</ui:UiBinder>