<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">

	<xsd:include schemaLocation="opencms://opencms-xmlcontent.xsd"/>

	<xsd:element name="SerialDateTests" type="OpenCmsSerialDateTests"/>

	<xsd:complexType name="OpenCmsSerialDateTests">
		<xsd:sequence>
			<xsd:element name="SerialDateTest" type="OpenCmsSerialDateTest" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="OpenCmsSerialDateTest">
		<xsd:sequence>
			<xsd:element name="Title" type="OpenCmsString" />
			<xsd:element name="Serial" type="OpenCmsSerialDate" />
		</xsd:sequence>
		<xsd:attribute name="language" type="OpenCmsLocale" use="required"/>
	</xsd:complexType>

	<xsd:annotation>
		<xsd:appinfo>
			<mappings>
				<mapping element="Serial" mapto="property:serialdate" />
			</mappings>
			<layouts>
				<layout element="Serial" widget="SerialDateWidget" configuration="" />
			</layouts>
			<searchsettings containerPageOnly="false"/>
		</xsd:appinfo>
	</xsd:annotation>

</xsd:schema>
