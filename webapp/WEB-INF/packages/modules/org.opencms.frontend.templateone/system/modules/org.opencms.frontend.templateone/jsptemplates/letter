		<% cms.includeSilent(null, "form_foot", true); %>
	</div>
	<div class="element"><span style="font-weight:bold; font-size: 1.2em">Mail Head</span><br>
		<% cms.includeSilent(null, "mail_head", true); %>
	</div>
