    <function>
        <description><![CDATA[
            <p>Converts the given path to a path relative to a base folder,
            but only if it actually is a sub-path of the latter,
            otherwise <code>null</code> is returned.</p>
            <p>The 1st parameter is the path, the 2nd paramerter is the base folder.</p>
        ]]></description>
        <name>getRelativeSubPath</name>
        <function-class>org.opencms.util.CmsStringUtil</function-class>
        <function-signature>
            java.lang.String getRelativeSubPath(java.lang.String, java.lang.String)
        </function-signature>
    </function>

    <function>
        <description><![CDATA[
            Returns true if the provided String is either <code>null</code> or contains only white spaces.
        ]]></description>
        <name>isEmptyOrWhitespaceOnly</name>
        <function-class>org.opencms.util.CmsStringUtil</function-class>
        <function-signature>
            boolean isEmptyOrWhitespaceOnly(java.lang.String)
        </function-signature>
    </function>

    <function>
        <description><![CDATA[
            <p>Checks if the first path is a prefix of the second path..</p>
            <p>This method is different compared to <code>String#startsWith</code>,
            because it considers <code>/foo/bar</code> to
            be a prefix path of <code>/foo/bar/baz</code>, but not of <code>/foo/bar42</code>.</p>
        ]]></description>
        <name>isPrefixPath</name>
        <function-class>org.opencms.util.CmsStringUtil</function-class>
        <function-signature>
            boolean isPrefixPath(java.lang.String, java.lang.String)
        </function-signature>
    </function>

    <function>
        <description><![CDATA[
            Checks if the given class name is a valid Java class name.
        ]]></description>
        <name>isValidJavaClassName</name>
        <function-class>org.opencms.util.CmsStringUtil</function-class>
        <function-signature>
            boolean isValidJavaClassName(java.lang.String)
        </function-signature>
    </function>

    <function>
        <description><![CDATA[
            Validates a value (1st parameter) against a regular expression (2nd parameter).
        ]]></description>
        <name>validateRegex</name>
        <function-class>org.opencms.jsp.util.CmsJspElFunctions</function-class>
        <function-signature>
            boolean validateRegex(java.lang.String, java.lang.String)
        </function-signature>
    </function>
</taglib>
