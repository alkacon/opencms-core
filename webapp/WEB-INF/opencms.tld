            <description><![CDATA[
              Name of the parameter.
            ]]></description>
            <name>name</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <description><![CDATA[
              Value of the parameter. Can alternatively be given in the tag's body.
            ]]></description>
